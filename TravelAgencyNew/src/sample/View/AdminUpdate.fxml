<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="455.0" prefWidth="652.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.AdminUpdateController">
    <children>
        <TableView fx:id="table" onKeyPressed="#handleKeyPressed" onMouseClicked="#getSelected" prefHeight="229.0" prefWidth="652.0">
            <columns>
                <TableColumn fx:id="SSNColumn" prefWidth="108.0" text="SSN" />
                <TableColumn fx:id="firstNameColumn" prefWidth="78.0" text="firstName" />
                <TableColumn fx:id="lastNameColumn" prefWidth="92.0" text="lastName" />
                <TableColumn fx:id="phoNumColumn" prefWidth="96.0" text="phoneNumber" />
                <TableColumn fx:id="emailColumn" prefWidth="78.0" text="email" />
                <TableColumn fx:id="passwordColumn" prefWidth="89.0" text="password" />
                <TableColumn fx:id="addressColumn" prefWidth="110.0" text="address" />
            </columns>
         <contextMenu>
            <ContextMenu fx:id="listContextMenu" onAction="#clicked">
              <items>
                <MenuItem mnemonicParsing="false" text="Remove" />
              </items>
            </ContextMenu>
         </contextMenu>
        </TableView>
        <Button fx:id="cancelButton" layoutX="544.0" layoutY="403.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="82.0" text="Cancel" />
        <Button fx:id="updateButton" layoutX="444.0" layoutY="359.0" mnemonicParsing="false" onAction="#update" prefHeight="25.0" prefWidth="82.0" text="Update" />
        <TextField fx:id="passwordTxtField" layoutX="384.0" layoutY="308.0" prefHeight="25.0" prefWidth="119.0" />
        <Button fx:id="backButton" layoutX="49.0" layoutY="403.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="76.0" text="Back" />
        <Label layoutX="25.0" layoutY="363.0" prefHeight="17.0" prefWidth="240.0" text="Edit your information and type in your SSN:" />
        <TextField fx:id="emailTxtField" layoutX="228.0" layoutY="307.0" prefHeight="25.0" prefWidth="119.0" />
        <TextField fx:id="ssnTxtField" layoutX="272.0" layoutY="359.0" promptText="SSN" />
        <TextField fx:id="phoneTxtField" layoutX="74.0" layoutY="307.0" prefHeight="25.0" prefWidth="113.0" />
        <Label layoutX="43.0" layoutY="283.0" prefHeight="17.0" prefWidth="88.0" text="PhoneNumber:" />
        <Label layoutX="206.0" layoutY="283.0" prefHeight="17.0" prefWidth="82.0" text="Email:" />
        <Label layoutX="378.0" layoutY="283.0" text="Password:" />
        <Label layoutX="517.0" layoutY="283.0" prefHeight="17.0" prefWidth="53.0" text="Address:" />
        <TextField fx:id="addressTxtField" layoutX="533.0" layoutY="308.0" prefHeight="25.0" prefWidth="119.0" />
    </children>
</AnchorPane>
