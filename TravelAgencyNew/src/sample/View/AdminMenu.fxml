<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="365.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.AdminMenuController">
    <children>
        <ImageView fitHeight="393.0" fitWidth="600.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Images/Airplane-Images-with-Beautiful-Picture-of-Flight-in-Sunset-500x313.jpg" />
            </image>
        </ImageView>
        <Button fx:id="editCustomerButton" layoutX="44.0" layoutY="72.0" mnemonicParsing="false" onAction="#updateCustomer" prefHeight="41.0" prefWidth="249.0" text="Update Customer Information">
         <font>
            <Font size="14.0" />
         </font></Button>
        <Button fx:id="viewPackageButton" layoutX="44.0" layoutY="145.0" mnemonicParsing="false" onAction="#editPackage" prefHeight="41.0" prefWidth="249.0" text=" View And Update Packages">
         <font>
            <Font size="14.0" />
         </font></Button>
        <Button fx:id="ViewBookedButton" layoutX="44.0" layoutY="216.0" mnemonicParsing="false" onAction="#editBookings" prefHeight="41.0" prefWidth="249.0" text=" View Booked And Remove Booking">
         <font>
            <Font size="14.0" />
         </font></Button>
        <Label layoutX="287.0" layoutY="-1.0" prefHeight="73.0" prefWidth="269.0" text="Admin options">
            <font>
                <Font size="26.0" />
            </font>
        </Label>
        <Button fx:id="backButton" layoutX="14.0" layoutY="328.0" mnemonicParsing="false" onAction="#back" text="Back" />
    </children>
</AnchorPane>
