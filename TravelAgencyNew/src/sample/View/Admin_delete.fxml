<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="611.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.Admin_deleteController">
    <children>
        <TableView fx:id="table" layoutX="-1.0" layoutY="-2.0" prefHeight="229.0" prefWidth="607.0">
            <columns>
                <TableColumn fx:id="ssnColumn" prefWidth="75.0" text="SSN" />
                <TableColumn fx:id="firstNameColumn" prefWidth="84.0" text="First Name" />
                <TableColumn fx:id="lastNameColumn" prefWidth="89.0" text="Last name" />
                <TableColumn fx:id="phoneNumberColumn" prefWidth="94.0" text="Phone number" />
                <TableColumn fx:id="emailColumn" prefWidth="73.0" text="Email" />
                <TableColumn fx:id="passwordColumn" prefWidth="104.0" text="Password" />
                <TableColumn fx:id="addressColumn" prefWidth="87.0" text="Adress" />
            </columns>
        </TableView>
        <Button layoutX="41.0" layoutY="282.0" mnemonicParsing="false" onAction="#select" text="View" />
        <Button layoutX="465.0" layoutY="282.0" mnemonicParsing="false" onAction="#remove" text="delete" />
        <TextField fx:id="deleteTextField" layoutX="287.0" layoutY="282.0" />
        <Button fx:id="backButton" layoutX="36.0" layoutY="343.0" mnemonicParsing="false" onAction="#backButton" text="Back" />
        <Label layoutX="257.0" layoutY="252.0" prefHeight="17.0" prefWidth="240.0" text="Enter customer SSN you want to remove" />
    </children>
</AnchorPane>
