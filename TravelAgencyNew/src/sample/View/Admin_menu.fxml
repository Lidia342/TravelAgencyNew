<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="365.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.Admin_menuController">
    <children>
        <ImageView fitHeight="393.0" fitWidth="600.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Images/Airplane-Images-with-Beautiful-Picture-of-Flight-in-Sunset-500x313.jpg" />
            </image>
        </ImageView>
        <Button fx:id="editCustomerButton" layoutX="70.0" layoutY="35.0" mnemonicParsing="false" onAction="#updateCustomer" prefHeight="41.0" prefWidth="132.0" text="Update customer info" />
        <Button fx:id="removeCustomerButton" layoutX="74.0" layoutY="90.0" mnemonicParsing="false" onAction="#deleteCustomer" prefHeight="41.0" prefWidth="124.0" text="Remove customer" />
        <Button fx:id="editBookingButton" layoutX="74.0" layoutY="146.0" mnemonicParsing="false" onAction="#editBookings" prefHeight="41.0" prefWidth="124.0" text="Edit bookings" />
        <Button fx:id="deleteBookingButton" layoutX="74.0" layoutY="196.0" mnemonicParsing="false" onAction="#deleteBooking" prefHeight="41.0" prefWidth="124.0" text="Delete bookings" />
        <Label layoutX="287.0" layoutY="-1.0" prefHeight="73.0" prefWidth="269.0" text="Admin options">
            <font>
                <Font size="26.0" />
            </font>
        </Label>
        <Button fx:id="viewPaymentButton" layoutX="74.0" layoutY="253.0" mnemonicParsing="false" onAction="#viewPayment" prefHeight="41.0" prefWidth="124.0" text="View payment" />
        <Button fx:id="backbutton" layoutX="14.0" layoutY="328.0" mnemonicParsing="false" onAction="#backButton" text="Log out" />
    </children>
</AnchorPane>
