<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="530.0" prefWidth="617.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.LogInController">
    <children>
        <ImageView fitHeight="493.0" fitWidth="499.0" layoutX="66.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Images/fgc_airline_logos_mobile_latestno-delta.jpg" />
            </image>
        </ImageView>
        <AnchorPane layoutX="106.0" layoutY="58.0" prefHeight="306.0" prefWidth="336.0" style="-fx-background-color: #ffffff; -fx-border-color: black; -fx-border-radius: 30;">
            <children>
                <Button fx:id="login" layoutX="168.0" layoutY="134.0" mnemonicParsing="false" onAction="#loginAdmin" prefHeight="34.0" prefWidth="110.0" text="Login as Admin" />
                <TextField fx:id="TfEmail" layoutX="81.0" layoutY="30.0" prefHeight="25.0" prefWidth="149.0" promptText="Please enter your email" />
                <Text layoutX="31.0" layoutY="217.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Don't have an Account?" />
                <Hyperlink fx:id="createOneHere" layoutX="188.0" layoutY="200.0" onAction="#createOneHere" text="CreateOneHere" />
                <Button fx:id="cancel" layoutX="18.0" layoutY="233.0" mnemonicParsing="false" onAction="#cancel" prefHeight="34.0" prefWidth="65.0" text="Cancel" />
                <Button layoutX="30.0" layoutY="134.0" mnemonicParsing="false" onAction="#loginCustomer" prefHeight="34.0" prefWidth="118.0" text="Log in as Customer" />
            <PasswordField fx:id="TfPassword" layoutX="82.0" layoutY="83.0" promptText="*******" />
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
