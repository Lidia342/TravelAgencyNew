<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="706.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.ViewBookedAdminController">
   <children>
      <TableView fx:id="table" layoutY="51.0" onKeyPressed="#handleKeyPressed" prefHeight="295.0" prefWidth="709.0">
        <columns>
            <TableColumn fx:id="bookingIdColumn" minWidth="0.0" prefWidth="0.0" text="Booking Id" />
            <TableColumn fx:id="packageNameColumn" prefWidth="109.99999749660492" text=" Package Name" />
          <TableColumn fx:id="firstNameColumn" prefWidth="117.3333511352539" text=" First Name" />
            <TableColumn fx:id="lastNameColumn" prefWidth="120.66661071777344" text=" Last Name" />
          <TableColumn fx:id="ssnColumn" prefWidth="133.3333740234375" text="SSN" />
            <TableColumn fx:id="dateColumn" prefWidth="134.00003051757812" text="  Date " />
            <TableColumn fx:id="priceColumn" prefWidth="93.33331298828125" text=" Price" />
        </columns>
         <contextMenu>
            <ContextMenu fx:id="listContextMenu" onAction="#clicked">
              <items>
                <MenuItem mnemonicParsing="false" text="Remove" />
              </items>
            </ContextMenu>
         </contextMenu>
      </TableView>
      <Button fx:id="backButton" layoutX="44.0" layoutY="356.0" mnemonicParsing="false" onAction="#back" prefHeight="29.0" prefWidth="57.0" text="Back" />
      <Button fx:id="cancelButton" layoutX="608.0" layoutY="355.0" mnemonicParsing="false" onAction="#cancel" prefHeight="29.0" prefWidth="64.0" text="Cancel" />
      <Label layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="241.0" text="Search: by booked package's name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="searchComboBox" layoutX="255.0" layoutY="12.0" onAction="#display" prefHeight="25.0" prefWidth="199.0" promptText="Package Name's" />
   </children>
</AnchorPane>
